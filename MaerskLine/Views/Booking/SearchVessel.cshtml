@model MaerskLine.Models.ViewSchedule
@{
    ViewBag.Title = "SearchVessel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2>Search Available Vessel</h2>


@using (Html.BeginForm("Search", "Booking", FormMethod.Get, new { @class = "form-horizontal", role = "form" }))

{
    @Html.AntiForgeryToken()
    <p class="alert-success">@ViewBag.Fail</p>
    @Html.ValidationSummary(true)


    <div class=" form-group">
        <span>Port Of Load</span>
        @Html.TextBox("lport")

    </div>

    <div class=" form-group">
        <span>Port Of Discharge</span>
        @Html.TextBox("dport")
    </div>

    <button type="submit" class="btn btn-primary" value="search">Search</button>
}


