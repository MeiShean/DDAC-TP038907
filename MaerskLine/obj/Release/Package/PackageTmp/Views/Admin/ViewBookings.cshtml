@model MaerskLine.Models.ViewBooking
@{
    ViewBag.Title = "ViewBookings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.AntiForgeryToken()
@Html.ValidationSummary(true)
<h2>View Bookings</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Booking Id</th>
            <th>Schedule Id</th>
            <th>Agent Name</th>
            <th>Customer Name</th>
            <th>Space occupied</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var v in Model.ListBooking)
        {

            <tr>
                <td>@v.Id</td>
                <td>@v.ScheduleId</td>
                <td>@v.AgentId</td>
                <td>@v.CustomerId</td>
                <td>@v.Space</td>

            </tr>
        }
    </tbody>
</table>



